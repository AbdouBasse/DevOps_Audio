{
  "dashboard": {
    "id": null,
    "title": "üéß AudioOps-Mastering Dashboard",   // Nom du dashboard
    "tags": ["audio", "devops", "monitoring"],   // Mots-cl√©s
    "timezone": "browser",
    "panels": [
      {
        "type": "graph",
        "title": "Latence du traitement audio",   // Graphique de latence
        "targets": [
          {
            "expr": "audio_mastering_latency_seconds", // M√©trique Prometheus fictive
            "legendFormat": "Latence (s)"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Secondes" },
          { "format": "short" }
        ]
      },
      {
        "type": "graph",
        "title": "Qualit√© audio (score IA)",      // Graphique qualit√©
        "targets": [
          {
            "expr": "audio_mastering_quality_score", // Score IA fictif
            "legendFormat": "Score qualit√©"
          }
        ],
        "yaxes": [
          { "format": "percent", "label": "Qualit√© (%)" },
          { "format": "short" }
        ]
      },
      {
        "type": "graph",
        "title": "Erreurs de mastering",          // Graphique erreurs
        "targets": [
          {
            "expr": "audio_mastering_errors_total", // Nombre d'erreurs
            "legendFormat": "Erreurs"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Nombre" },
          { "format": "short" }
        ]
      }
    ]
  }
}
